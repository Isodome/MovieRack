<Window x:Class="WinMovieRack.GUI.ErrorMessage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ErrorMessage" Height="auto" Width="300" WindowStyle="ToolWindow" ResizeMode="NoResize" SizeToContent="WidthAndHeight" Topmost="True" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Name="stackPanel2" Margin="50,0">
            <TextBlock Text="Error Message" Height="Auto" Name="errorMessage" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,20,0,0" />
            <Button Content="OK" Height="23" Name="okButton" Width="75" Margin="0,40,0,20" Click="okButton_Click" />
        </StackPanel>
    </Grid>
</Window>
